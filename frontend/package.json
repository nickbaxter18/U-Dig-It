{
  "name": "udigit-rentals-frontend",
  "version": "1.0.0",
  "description": "U-Dig It Rentals - Next.js Frontend for Kubota SVL-75 Rental Platform",
  "main": "index.js",
  "scripts": {
    "dev": "bash ../start-frontend-clean.sh",
    "dev:raw": "next dev",
    "build": "next build",
    "build:check": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit --noEmitOnError false",
    "type-check:verbose": "tsc --noEmit --noEmitOnError false --pretty true",
    "analyze": "cross-env ANALYZE=true next build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "size": "size-limit",
    "size:why": "size-limit --why",
    "knip": "knip",
    "knip:production": "knip --production",
    "unused": "knip",
    "security:scan": "snyk code test --severity-threshold=high",
    "security:deps": "snyk test",
    "security:all": "pnpm security:scan && pnpm security:deps",
    "test": "bash scripts/test.sh",
    "test:watch": "bash scripts/test.sh --watch",
    "test:coverage": "bash scripts/test.sh --coverage",
    "test:ci": "bash scripts/test-ci.sh",
    "test:validation": "vitest src/lib/__tests__/validation.test.ts --run",
    "test:cache": "pnpm vitest src/lib/__tests__/cache.test.ts --run",
    "test:lib": "pnpm vitest src/lib/__tests__ --run",
    "test:components": "pnpm vitest src/components/__tests__ --run",
    "test:api": "pnpm vitest src/app/api/__tests__ --run",
    "test:admin": "pnpm vitest src/components/admin/__tests__/BookingsTable.test.tsx src/components/admin/__tests__/RevenueChart.test.tsx src/components/admin/__tests__/CustomerEditModal.test.tsx src/components/admin/__tests__/RefundModal.test.tsx src/app/api/admin/__tests__/payments-refund.test.ts src/app/api/admin/__tests__/contracts-generate.test.ts src/app/api/admin/__tests__/analytics-export.test.ts --run",
    "test:quality": "node scripts/quality-gate.js",
    "test:performance": "lhci autorun --config=lighthouserc.js",
    "test:e2e": "playwright test",
    "test:e2e:admin": "playwright test tests/e2e/admin",
    "test:e2e:admin:ui": "playwright test tests/e2e/admin --ui",
    "test:e2e:admin:debug": "playwright test tests/e2e/admin --debug",
    "test:accessibility": "playwright test --config=playwright.config.ts --project=accessibility",
    "test:bundle-analyze": "ANALYZE=true pnpm build",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "clean": "rm -rf .next .turbo node_modules/.cache",
    "reconcile:stripe": "tsx scripts/reconcile-stripe.ts",
    "policy:drift": "tsx scripts/check-policy-drift.ts",
    "coverage:delta": "tsx scripts/coverage-delta.ts",
    "precommit": "bash ../.husky/pre-commit",
    "quality:all": "pnpm lint && pnpm type-check && pnpm test && pnpm security:scan",
    "quality:fast": "pnpm lint && pnpm type-check",
    "ci": "pnpm quality:all && pnpm build"
  },
  "keywords": [
    "nextjs",
    "rental",
    "kubota",
    "equipment",
    "booking",
    "react",
    "typescript"
  ],
  "author": "U-Dig It Rentals Inc.",
  "license": "ISC",
